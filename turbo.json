{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".next/**", "dist/**"]
        },
        "build:pkg": {
            "cache": false,
            "dependsOn": ["^build:pkg"],
            "outputs": [".next/**", "dist/**"]
        },
        "build:twa": {
            "dependsOn": ["^build:pkg"],
            "outputs": [".next/**", "dist/**"]
        },
        "build:web": {
            "dependsOn": ["^build:pkg"],
            "outputs": [".next/**", "dist/**"]
        },
        "build:extension": {
            "cache": false,
            "dependsOn": ["^build:pkg"],
            "outputs": [".next/**", "dist/**"]
        },
        "build:desktop": {
            "cache": false,
            "dependsOn": ["^build:pkg"],
            "outputs": [".next/**", "dist/**"]
        },
        "dev": {
            "dependsOn": ["^dev"],
            "outputs": [".next/**", "dist/**"]
        }
    }
}
